{
  "name": "nestjs-firebase-utilities",
  "version": "0.1.2",
  "description": "NestJS Package including abstractions for firebase and passport strategies",
  "author": "Renan Zelaya <renan_zelaya@hotmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rzdevelop/nestjs-firebase-utilities.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm publish"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "cqrs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-strategy": "^0.2.35",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/testing": "^7.6.15",
    "express": "^4.17.1",
    "firebase-admin": "^9.8.0",
    "nestjs-utilities": "^0.1.6",
    "passport-jwt": "^4.0.0",
    "passport-strategy": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "firebase-admin": "^9.8.0",
    "nestjs-utilities": "^0.1.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.1"
  }
}
