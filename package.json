{
  "name": "nestjs-firebase-utilities",
  "version": "0.1.6",
  "description": "NestJS Package including abstractions for firebase and passport strategies",
  "author": "Renan Zelaya <renan_zelaya@hotmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rzdevelop/nestjs-firebase-utilities.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' -c .eslintrc.js",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && npm publish"
  },
  "keywords": [
    "nestjs",
    "firebase",
    "passport"
  ],
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/terminus": "^7.1.2",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-strategy": "^0.2.35",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "joi": "^17.4.0",
    "nestjs-utilities": "^0.1.21",
    "prettier": "^2.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-strategy": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.0.0",
    "ts-jest": "^26.5.6",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "firebase-admin": "^9.8.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/passport": "^7.1.5",
    "nestjs-utilities": "^0.1.21",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0"
  }
}
